     <meta property='og:image' content="<%= image_url('project_pic.png') %>"/>
     <meta property="og:description"
       content="An image-based social media app inspired by Pinterest.This website gives users an enjoyable experience by mainly providing visuals that are visually pleasing."
     />
     <meta property="og:url"
       content="https://pinspo.herokuapp.com/#/"
     />
    <title>Pinspo</title>
    <%= csrf_meta_tags %>
