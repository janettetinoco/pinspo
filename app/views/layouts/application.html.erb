    <%= favicon_link_tag 'favicon.png' %>
    <% if current_user %>
      <script type="text/javascript">
          window.currentUser = {
            "id": <%= current_user.id %>,
            "username" : "<%= current_user.username %>",
            "email": "<%= current_user.email %>"
          }
      </script>
    <% end %>
    
